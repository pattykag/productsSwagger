{
  "name": "productsSwagger",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/audit-logging": "^5.5.5",
    "@sap/cds": ">=5",
    "@sap/logging": "^6.1.3",
    "@sap/xsenv": "^3.3.2",
    "@sap/xssec": "^3.2.13",
    "cds-swagger-ui-express": "^0.5.0",
    "cf-nodejs-logging-support": "^6.13.1",
    "cors": "^2.8.5",
    "express": "^4",
    "hdb": "^0.19.0",
